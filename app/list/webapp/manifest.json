{
  "_version": "1.76.0",
  "sap.app": {
    "id": "list",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.0",
      "toolsId": "07d9df86-1557-4c4c-8bf1-9f6f4e96b046"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/po/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {},
        "sap.fe.macros": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "list.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "POHeadersList",
          "target": "POHeadersList"
        },
        {
          "pattern": "POHeaders({key}):?query:",
          "name": "POHeadersObjectPage",
          "target": "POHeadersObjectPage"
        },
        {
          "pattern": "POHeaders({key})/items({key2}):?query:",
          "name": "POItemsObjectPage",
          "target": "POItemsObjectPage"
        }
      ],
      "targets": {
        "POHeadersList": {
          "type": "Component",
          "id": "POHeadersList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/POHeaders",
              "variantManagement": "Page",
              "navigation": {
                "POHeaders": {
                  "detail": {
                    "route": "POHeadersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "enableExport": true,
                    "enableMassEdit": {},
                    "selectionLimit": 1,
                    "creationMode": {
                      "name": "CreationDialog"
                    },
                    "personalization": {
                      "column": true
                    }
                  },
                  "actions": {
                    "sendmail": {
                      "press": "list.ext.controller.Sendmail.sendmail",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "{i18n>sendMail}"
                    }
                  }
                },
                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                  "filterFields": {
                    "Materials": {
                      "label": "{i18n>materials}",
                      "property": "items/material",
                      "template": "list.ext.fragment.Mat",
                      "required": false,
                      "position": {
                        "placement": "Before",
                        "anchor": "supplier"
                      }
                    }
                  }
                },
                "/POItems/@com.sap.vocabularies.UI.v1.LineItem#tableView": {
                  "tableSettings": {
                    "enableMassEdit": {},
                    "enableExport": true,
                    "type": "GridTable",
                    "widthIncludingColumnHeader": true
                  }
                }
              },
              "hideFilterBar": false,
              "initialLoad": "Enabled",
              "views": {
                "paths": [
                  {
                    "key": "tableView",
                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#tableView"
                  },
                  {
                    "key": "tableView1",
                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#tableView",
                    "entitySet": "POItems"
                  }
                ]
              }
            }
          }
        },
        "POHeadersObjectPage": {
          "type": "Component",
          "id": "POHeadersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/POHeaders",
              "navigation": {
                "items": {
                  "detail": {
                    "route": "POItemsObjectPage"
                  }
                }
              },
              "sectionLayout": "Page",
              "content": {
                "footer": {
                  "actions": {
                    "approve": {
                      "press": "list.ext.controller.Approve.approve",
                      "visible": true,
                      "enabled": true,
                      "text": "{i18n>approve}"
                    },
                    "reject": {
                      "press": "list.ext.controller.Reject.Reject",
                      "visible": true,
                      "enabled": true,
                      "text": "{i18n>rejected}",
                      "position": {
                        "placement": "After",
                        "anchor": "approve"
                      }
                    }
                  }
                },
                "header": {
                  "anchorBarVisible": true
                },
                "body": {
                  "sections": {
                    "POItems": {
                      "template": "list.ext.fragment.POItems",
                      "position": {
                        "placement": "After",
                        "anchor": "GeneratedFacet1"
                      },
                      "title": "Purchase Order Items"
                    }
                  }
                }
              },
              "variantManagement": "None"
            }
          }
        },
        "POItemsObjectPage": {
          "type": "Component",
          "id": "POItemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/POHeaders/items",
              "sectionLayout": "Page"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
